{
  "name": "shopping-cart-backend",
  "version": "1.0.0",
  "description": "",
  "main": "dist/app.js",
  "engines": {
    "node": "18"
  },
  "scripts": {
    "dev": "tsx watch src/app.ts",
    "build": "rm -rf ./dist && tsup src/server.ts --format cjs --minify --dts --clean --sourcemap --legacy-output",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "prettier": "prettier --write --loglevel silent src",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --clear-screen=true",
    "test:coverage": "jest --coverage",
     "seed": "tsx src/scripts/seed.ts"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Icaro Oliveira",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.16.2",
    "express": "^4.19.2",
    "node-cron": "^3.0.3",
    "pino": "^9.2.0",
    "prisma": "^5.16.2",
    "sqlite3": "^5.1.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.0",
    "@typescript-eslint/parser": "^7.16.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "pino-pretty": "^11.2.1",
    "prettier": "^3.3.3",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.2",
    "tsx": "^4.16.2",
    "typescript": "^5.5.3"
  }
}
